<h2><%=t :Listing %>&nbsp;<%=t :township_i %></h2>

<table cellspacing=15 >
  <tr>
    <td><%=t 'Name' %></td>
  </tr>
  <% @townships.each do |township| %>
    <tr>
      <td><%= township.name %></td>
      <td><%= link_to t('Show'), township %></td>
      <td><%= link_to t('Edit'), edit_township_path(township) %></td>
      <td><%= link_to t('Destroy'), township, method: :delete, data: { confirm: t('Are_you_sure?') } %></td>
    </tr>
  <% end %>
</table>

<br>

&nbsp;&nbsp;&nbsp;<%= link_to t('Home'), root_path %>
&nbsp;&nbsp;&nbsp;<%= link_to t('New'), new_township_path %>
