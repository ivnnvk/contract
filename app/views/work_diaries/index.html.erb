<h1>Listing work_diaries</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @work_diaries.each do |work_diary| %>
      <tr>
        <td><%= link_to 'Show', work_diary %></td>
        <td><%= link_to 'Edit', edit_work_diary_path(work_diary) %></td>
        <td><%= link_to 'Destroy', work_diary, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Work diary', new_work_diary_path %>
