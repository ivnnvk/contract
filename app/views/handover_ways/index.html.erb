<h2><%=t :Listing %>&nbsp;<%=t :handover_ways_u %></h2>

<table cellspacing=15 >
  <tr>
    <td><%=t 'Name' %></td>
  </tr>
  <% @handover_ways.each do |handover_way| %>
    <tr>
      <td><%= handover_way.name %></td>
      <td><%= link_to t('Show'), handover_way %></td>
      <td><%= link_to t('Edit'), edit_handover_way_path(handover_way) %></td>
      <td><%= link_to t('Destroy'), handover_way, method: :delete, data: { confirm: t('Are_you_sure?') } %></td>
    </tr>
  <% end %>
</table>

<br>

&nbsp;&nbsp;&nbsp;<%= link_to t('Home'), root_path %>
&nbsp;&nbsp;&nbsp;<%= link_to t('New'), new_handover_way_path %>
