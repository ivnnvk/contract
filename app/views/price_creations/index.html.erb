<h1>Listing price_creations</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <tr>
      <td><%=t 'name' %></td>
    </tr>
    <% @price_creations.each do |price_creation| %>
      <tr>
        <td><%= price_creation.name %></td>
        <td><%= link_to 'Show', price_creation %></td>
        <td><%= link_to 'Edit', edit_price_creation_path(price_creation) %></td>
        <td><%= link_to 'Destroy', price_creation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Home', root_path %>
<%= link_to 'New', new_price_creation_path %>
