<h2><%=t :Listing %>&nbsp;<%=t :price_creation_y %></h2>

<table cellspacing=15 >
  <tr>
    <td><%=t 'Name' %></td>
  </tr>
  <% @price_creations.each do |price_creation| %>
    <tr>
      <td><%= price_creation.name %></td>
      <td><%= link_to t('Show'), price_creation %></td>
      <td><%= link_to t('Edit'), edit_price_creation_path(price_creation) %></td>
      <td><%= link_to t('Destroy'), price_creation, method: :delete, data: { confirm: t('Are_you_sure?') } %></td>
    </tr>
  <% end %>
</table>

<br>

&nbsp;&nbsp;&nbsp;<%= link_to t('Home'), root_path %>
&nbsp;&nbsp;&nbsp;<%= link_to t('New'), new_price_creation_path %>
